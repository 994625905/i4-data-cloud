<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.i4.data.cloud.core.mapper.PartyActivityImageLikeMapper">

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        t.id as id, 
        t.image_id as imageId, 
        t.create_user_id as createUserId, 
        t.create_time as createTime,
        t.type as type
    </sql>

    <select id="selectPage" parameterType="cn.i4.data.cloud.core.entity.dto.PartyActivityImageLikeDto"
            resultType="cn.i4.data.cloud.core.entity.view.PartyActivityImageLikeView">
        SELECT
        <include refid="Base_Column_List" />
        FROM t_party_activity_image_like AS t

    </select>

</mapper>